import java.util.Scanner;
public class Solution {
    public static String compareIfElse(int a, int b){
        // Write your code here
       
        // int a, b;
        if(a>b)
            return "greater";
        else if (a<b)
            return "smaller";
        else
            return "equal";
    }
    
}
