IF ELSE STATEMENT

import java.util.Scanner;
public class Fundamental{
    public static void main(String[] args){
        Scanner sc= new Scanner(System.in);
        int a = sc.nextInt();
        
        if(a>=18){
            System.out.println("its ADULT");

        }else{
            System.out.println("its not ADULT");
        }
        
    }
}
